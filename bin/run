#!/bin/bash

echo -n -e "\033]0;mora.run\007"

osascript -e '
  tell app "Terminal"
    do script "cd /Users/gage/Uber/server; node run 1"
  end tell
'

osascript -e '
  tell app "Terminal"
    do script "cd /Users/gage/Uber/server; node run 100"
  end tell
'

osascript -e 'tell application "Terminal" to close (every window whose name contains "mora.run")' &
exit